    "vite-plugin-env-compatible": "^1.0.0",
    "vite-plugin-html-env": "^1.0.2-0",
    "vite-plugin-vue2": "^1.4.0",
    "vue-eslint-parser": "^7.6.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14"
  },
  "pre-commit": "lint",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(js)$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
