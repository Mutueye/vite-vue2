    "lodash": "^4.17.21",
    "markdown-it": "^12.0.4",
    "markdown-it-github-toc": "^3.2.4",
    "uuid": "^8.3.2",
    "vue": "^2.6.12",
    "vue-router": "^3.2.0",
    "vue-scrollto": "^2.20.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@vitejs/plugin-legacy": "^1.4.3",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^27.0.6",
    "jsdom": "^16.6.0",
    "pre-commit": "^1.2.2",
