    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,vue,css,scss,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix "
  },
  "dependencies": {
    "element-ui": "^2.15.1",
